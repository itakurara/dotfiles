syntax enable
set number
set ruler
set list
set listchars=tab:»-,trail:-,eol:↲,extends:»,precedes:«,nbsp:%
set incsearch
set hlsearch
set showmatch
set whichwrap=h,l
""set nowrapscan
""set ignorecase
""set smartcase
""set hidden
""set history=2000
""set autoindent
""set expandtab
""set tabstop=2
""set shiftwidth=2
""set clipboard=unnamed,autoselect
set lines=55
set columns=230
colorscheme monokai
set guifont=rounded-mplus-1mn-regular:h15
""set cursorline
""set nocompatible
""set laststatus=2
""set statusline=%<%f\ %m%r%h%w%{'['.(&fenc!=''?&fenc:&enc).']['.&ff.']'}%=%l,%c%V%8P}
""inoremap { {}<LEFT>
""inoremap [ []<LEFT>
""inoremap ( ()<LEFT>
""inoremap " ""<LEFT>
""inoremap ' ''<LEFT>
"imap <c-j> <esc>
